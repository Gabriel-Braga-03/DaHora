unit Model.Turma;

interface
    uses Model.Entidade;

    type
        TDiaS = (Segunda, Terça, Quarta, Quita, Sexta);
        TTurma = class (TEntidade)
            private
                Fserie: Byte;
                Fcomplemento: String;

                Fds: TDiaS;
            public
                property serie: Byte read Fserie write Fserie;
                property complemento: String read Fcomplemento write Fcomplemento;

                property ds: TDiaS read Fds write Fds;
        end;

implementation

end.
